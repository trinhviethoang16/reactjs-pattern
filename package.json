{
  "name": "udt-core",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^0.21.4",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "postcss-normalize": "^10.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.4.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "eslint": "eslint \"*/**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint --fix \"*/**/*.{js,ts,tsx}\"",
    "eslint:ci": "eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "prettier:fix": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "lint:fix": "yarn eslint:fix && yarn prettier:fix",
    "preversion": "yarn config set version-tag-prefix \"release-v\"",
    "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.3",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "prettier": "^2.4.1"
  }
}
